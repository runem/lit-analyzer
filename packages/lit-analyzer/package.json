{
	"name": "lit-analyzer",
	"version": "1.2.1",
	"description": "CLI that type checks bindings in lit-html templates",
	"author": "runem",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/runem/lit-analyzer.git"
	},
	"keywords": [
		"lit-html",
		"lit",
		"lit-element",
		"javascript",
		"typescript",
		"web components",
		"web",
		"components",
		"tagged",
		"template"
	],
	"bin": {
		"lit-analyzer": "cli.js"
	},
	"scripts": {
		"clean": "rimraf lib",
		"build": "wireit",
		"readme": "readme generate -i readme.blueprint.md -c readme.config.json",
		"test": "wireit",
		"test:ava": "wireit",
		"test:all": "npm test",
		"test:watch": "ava --watch --color",
		"check-version": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --build --pretty",
			"files": [
				"src/**/*",
				"tsconfig.json"
			],
			"output": [
				"lib",
				"test",
				"scripts",
				"index.js",
				"index.d.ts",
				"index.d.ts.map",
				".tsbuildinfo"
			],
			"clean": "if-file-deleted"
		},
		"test": {
			"dependencies": [
				"test:ava",
				"check-version"
			]
		},
		"test:ava": {
			"dependencies": [
				"build"
			],
			"command": "ava --color",
			"output": [],
			"files": []
		},
		"check-version": {
			"dependencies": [
				"build"
			],
			"files": [
				"package.json",
				"scripts/check-version.js"
			],
			"command": "node scripts/check-version.js"
		}
	},
	"main": "index.js",
	"files": [
		"/lib/",
		"index.{js|js.map|d.ts|d.ts.map}",
		"cli.js"
	],
	"dependencies": {
		"chalk": "^2.4.2",
		"didyoumean2": "4.1.0",
		"fast-glob": "^3.2.11",
		"parse5": "5.1.0",
		"ts-simple-type": "~2.0.0-next.0",
		"vscode-css-languageservice": "4.3.0",
		"vscode-html-languageservice": "3.1.0",
		"vscode-web-custom-data": "^0.3.0",
		"web-component-analyzer": "2.0.0-next.4"
	},
	"devDependencies": {
		"@appnest/readme": "^1.2.7",
		"@types/node": "^14.0.13",
		"ava": "^3.8.2",
		"rimraf": "^3.0.2",
		"tslib": "^2.0.0",
		"typescript": "~4.4.3",
		"typescript-3.6": "npm:typescript@~3.6.4",
		"typescript-3.7": "npm:typescript@~3.7.4",
		"typescript-3.8": "npm:typescript@~3.8.3",
		"typescript-3.9": "npm:typescript@~3.9.3",
		"wireit": "^0.1.1"
	},
	"ava": {
		"cache": true,
		"timeout": "200s",
		"extensions": [
			"js"
		],
		"snapshotDir": "test/snapshots/results",
		"files": [
			"test/**/*.js",
			"!test/helpers/**/*"
		]
	},
	"contributors": [
		{
			"name": "Rune Mehlsen",
			"url": "https://twitter.com/runemehlsen",
			"img": "https://avatars2.githubusercontent.com/u/5372940?s=460&v=4"
		},
		{
			"name": "Andreas Mehlsen",
			"url": "https://twitter.com/andreasmehlsen",
			"img": "https://avatars1.githubusercontent.com/u/6267397?s=460&v=4"
		},
		{
			"name": "You?",
			"img": "https://joeschmoe.io/api/v1/random",
			"url": "https://github.com/runem/lit-analyzer/blob/master/CONTRIBUTING.md"
		}
	]
}
